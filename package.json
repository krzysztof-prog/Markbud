    "db:migrate:status": "pnpm --filter @akrobud/api db:migrate:status",
    "db:push": "pnpm --filter @akrobud/api db:push",
    "db:seed": "pnpm --filter @akrobud/api db:seed",
    "db:studio": "pnpm --filter @akrobud/api db:studio",
    "db:reset": "pnpm --filter @akrobud/api db:reset",
    "lint": "pnpm run --parallel lint",
    "clean": "pnpm run --parallel clean",
    "kill": "node scripts/kill-dev-servers.js",
    "bd": "C:\\Users\\krzys\\AppData\\Local\\bd\\bd.exe",
    "bd:list": "C:\\Users\\krzys\\AppData\\Local\\bd\\bd.exe list",
